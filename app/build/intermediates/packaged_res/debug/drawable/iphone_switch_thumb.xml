<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/iphone_switch_thumb_color_checked" />
            <size android:width="24dp" android:height="24dp" />
        </shape>
    </item>
    <item>
        <shape android:layout_width="wrap_content" android:layout_height="wrap_content" android:shape="oval">
            <solid android:color="@color/iphone_switch_thumb_color_unchecked" />
            <size android:width="24dp" android:height="24dp" />
        </shape>
    </item>
</selector>
